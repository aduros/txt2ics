{
  "name": "txt2ics",
  "version": "1.0.0",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "txt2ics": "./dist/cli.mjs"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --sourcemap --dts",
    "prepack": "npm run build -- --clean",
    "prepare": "husky",
    "release": "bump --commit --tag --push",
    "start": "npm run build -- --watch",
    "test": "jest",
    "test:lint": "eslint .",
    "test:types": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "jest --no-coverage --bail --passWithNoTests --findRelatedTests"
    ]
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.6",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.9",
    "eslint-plugin-prettier": "^5.1.3",
    "glob": "^11.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "version-bump-prompt": "^6.1.0"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "ohash": "^1.1.3",
    "timezones-ical-library": "^1.8.3",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "ical-generator": "^7.2.0",
    "openai": "^4.55.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aduros/txt2ics"
  }
}
